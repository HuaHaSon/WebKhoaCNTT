                                        <th>Nội dung</th>
                                        <th>flag</th>
                                        <th> Chức năng </th>
                                    </tr>
                                </thead>
                                <tbody>
                                    @foreach (var item in Model.listbomon)
                                    {
                                        <tr>
                                            <td class="mabomon">@item.MaBM</td>
                                            <td>@item.TenBM</td>
                                            <td>@Html.Raw(item.Noidung)</td>
                                            <td>
                                                <a class="btnbomonflag" data-mabomon="@item.MaBM" hres="#">
                                                    @if (@item.Flag == true)
                                                    {
