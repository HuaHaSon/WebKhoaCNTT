                                                    <label class="col-md-12">Mail</label>
                                                    <div class="col-md-12">
                                                        @Html.TextBoxFor(Model => Model.giaovien.Diachimail, "", new { @type = "mail", @class = "form-control mailgiaovienedit", @placeholder = "...@mail.com", required = "required" })
                                                    </div>
                                                </div>
                                            </div>
                                        </div>


                                        <div class="modal-footer">

                                            <button type="button" class="btn btn-default" data-dismiss="modal">Đóng</button>
                                            <button type="button" id="btnsuagiaovien" class="btn btn-success waves-effect waves-light m-r-30">Sửa</button>

                                        </div>
                                    </form>


                                </div>

                            </div>
                        </div>
                        <!-- Thanh ẩn -->
                        <div class="panel">
                            Ẩn cột: <a class="toggle-vis" id="tgiaovien" data-column="1">Mã bộ môn</a> / <a class="toggle-vis" id="tgiaovien" data-column="2">Tên giáo viên</a> / <a class="toggle-vis" id="tgiaovien" data-column="3">Chức vụ</a> / <a class="toggle-vis" id="tgiaovien" data-column="4">Chức danh</a> / <a class="toggle-vis" id="tgiaovien" data-column="5">Thông tin giáo viên</a> / <a class="toggle-vis" id="tgiaovien" data-column="6">Mail</a> / <a class="toggle-vis" id="tgiaovien" data-column="7">Flag</a> / <a class="toggle-vis" id="tgiaovien" data-column="8">Chức năng</a>
                        </div>
                        <!-- table -->
                        <div class="table-responsive">
                            <table id="giaovienTable" class="table table-striped">
                                <thead>
                                    <tr>
                                        <th>Mã giáo viên</th>
                                        <th>Mã bộ môn</th>
                                        <th>Tên giáo viên</th>
                                        <th>Chức vụ</th>
                                        <th>Chức danh</th>
                                        <th>Thông tin giáo viên</th>
                                        <th>Mail</th>
                                        <th>Flag</th>
                                        <th> Chức năng </th>
                                    </tr>
                                </thead>
                                <tbody>
                                    @foreach (var item in Model.listgiaovien)
                                    {
                                        <tr>
                                            <td class="magiaovien">@item.MaGV</td>
                                            <td>@item.MaBM</td>
                                            <td>@item.TenGV</td>
                                            <td>@item.Chucvu</td>
                                            <td>@item.Chucdanh</td>
                                            <td>@item.ThongtinGV1</td>
                                            <td>@item.Diachimail</td>
                                            <td>
                                                <a class="btngiaovienflag" data-magiaovien="@item.MaGV" hres="#">
                                                    @if (@item.Flag == true)
                                                    {
                                                        <span class="text-info"> Không khóa </span>
                                                    }
                                                    else
                                                    {
                                                        <span class="text-danger"> Khóa </span>
                                                    }
                                                </a>
                                            </td>
                                            <td>
                                                <button class="btnloadgiaovien" type="button" data-toggle="modal" data-target="#suagiaovien" style="background-color:transparent;border:none">
                                                    <i class="fa fa-pencil-square-o" style="color:red"> </i>
                                                </button>
                                            </td>

                                        </tr>
                                    }


                                </tbody>
                            </table>
                        </div>
                    </div>
                </div>
            </div>
