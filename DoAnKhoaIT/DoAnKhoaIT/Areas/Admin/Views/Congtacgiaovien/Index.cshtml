                            <div class="table-responsive">
                                <table id="congtacTable" class="table table-striped">
                                    <thead>
                                        <tr>
                                            <th>Mã công tác</th>
                                            <th>Tên công tác</th>
                                            <th>Flag</th>
                                            <th> Chức năng </th>
                                        </tr>
                                    </thead>
                                    <tbody>
                                        @foreach (var item in Model.listcongtacgiaovien)
                                        {
                                            <tr>
                                                <td class="macongtac">@item.MaCT</td>
                                                <td>@item.TenCT</td>
                                                <td>
                                                    <a class="btncongtacflag" data-macongtac="@item.MaCT" hres="#">
                                                        @if (@item.Flag == true)
                                                        {
                                                            <span class="text-info"> Không khóa </span>
                                                        }
                                                        else
                                                        {
                                                            <span class="text-danger"> Khóa </span>
                                                        }
                                                    </a>
                                                </td>
                                                <td>
                                                    <button class="btnloadcongtac" type="button" data-toggle="modal" data-target="#suacongtac" style="background-color:transparent;border:none">
                                                        <i class="fa fa-pencil-square-o" style="color:red"> </i>
                                                    </button>
                                                </td>

                                            </tr>
                                        }


                                    </tbody>
                                </table>
                            </div>
