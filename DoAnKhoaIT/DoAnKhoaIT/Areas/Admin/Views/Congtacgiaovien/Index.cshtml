                                                        <label class="col-md-12"> Mã công tác giáo viên</label>
                                                        <div class="col-md-12">
                                                            @Html.TextBoxFor(model => model.congtacgiaovien.MaCT, "", new { @type = "text", @class = "form-control macongtacedit", @placeholder = "Mã công tác giáo viên", @required = "required", @readonly = "readonly" })

                                                        </div>
                                                    </div>
                                                    <div class="form-group">
                                                        <label class="col-md-12"> Tên công tác giáo viên</label>
                                                        <div class="col-md-12">
                                                            @*@Html.TextBoxFor(model => model.baidang.Maloai, "", new { @type = "text", @class = "form-control maloaiadd", @placeholder = "Mã loại", @required = "required" })*@
                                                            @Html.TextBoxFor(Model => Model.congtacgiaovien.TenCT, "", new { @class = "form-control tencongtacedit", @placeholder = "Tên công tác giáo viên", required = "required" })
                                                        </div>
                                                    </div>
                                                </div>
                                            </div>


                                            <div class="modal-footer">

