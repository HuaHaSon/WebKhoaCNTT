@model Model.ViewModel.CongtacgiaovienModel
@{
    ViewBag.Title = "Index";
    Layout = "~/Areas/Admin/Views/Shared/Layout.cshtml";
}

@section jsFooter{


    <script src="~/Assets/Admin/js/admin/controller/Congtacgiaovienadmin.js"></script>

}

    <div class="container-fluid">
        <div class="row bg-title">
            <div class="col-lg-3 col-md-4 col-sm-4 col-xs-12">
                <h4 class="page-title">Quản lý</h4>
            </div>
            <div class="col-lg-9 col-sm-8 col-md-8 col-xs-12">
                <button class="right-side-toggle waves-effect waves-light btn-info btn-circle pull-right m-l-20"><i class="ti-settings text-white"></i></button>

                <ol class="breadcrumb">
                    <li><a href="#">Quản lý</a></li>
                    <li class="active">Công tác giáo viên</li>
                </ol>
            </div>
            <!-- /.col-lg-12 -->
        </div>
        <!-- /row -->
        <div class="row">
            <div class="col-sm-12">
                <div class="white-box" id="tabcongtacgiaovien">
                    <ul class="nav customtab nav-tabs" role="tablist">
                        <li role="presentation" id="tabcongtac" class="active"><a href="#congtac1" aria-controls="congtac" role="tab" data-toggle="tab" aria-expanded="true"><i class="ti-home"></i><span class="hidden-xs">Công tác</span></a></li>
                        <li role="presentation" id="tabchitietcongtac" class=""><a href="#chitietcongtac1" aria-controls="chitietcongtac" role="tab" data-toggle="tab" aria-expanded="false"><i class="ti-user"></i> <span class="hidden-xs">Chi tiết công tác</span></a></li>
                    </ul>
                    <!-- Tab panes -->
                    <!-- bảng công tác giáo viên -->

                    <div class="tab-content">
                        <div role="tabpanel" class="tab-pane fade active in" id="congtac1">
                            <!-- button thêm -->
                            <div style="margin-bottom: 15px">
                                <button class="btn btn-success" data-toggle="modal" data-target="#themcongtac"> Thêm công tác</button>
                            </div>
                            <!-- modal -->

                            <div id="themcongtac" class="modal fade" role="dialog">
                                <div class="modal-dialog  modal-lg">

                                    <!-- Modal content-->
                                    <div class="modal-content">
                                        <div class="modal-header">
                                            <button type="button" class="close" data-dismiss="modal">&times;</button>
                                            <h4 class="modal-title">Thêm công tác giáo viên</h4>
                                        </div>

                                        <form data-toggle="validator" class="form-horizontal">
                                            <div class="modal-body">
                                                <div class="white-box col-lg-12 m-b-0">
                                                    <h3 class="box-title m-b-0">Thông tin công tác giáo viên</h3>
                                                    <div class="form-group">
                                                        <label class="col-md-12"> Mã công tác giáo viên </label>
                                                        <div class="col-md-12">
                                                            @Html.TextBoxFor(model => model.congtacgiaovien.MaCT, "", new { @type = "text", @class = "form-control macongtacadd", @placeholder = "Mã công tác giáo viên", @required = "required" })

                                                        </div>
                                                    </div>
                                                    <div class="form-group">
                                                        <label class="col-md-12"> Tên công tác giáo viên</label>
                                                        <div class="col-md-12">
                                                            @*@Html.TextBoxFor(model => model.baidang.Maloai, "", new { @type = "text", @class = "form-control maloaiadd", @placeholder = "Mã loại", @required = "required" })*@
                                                            @Html.TextBoxFor(Model => Model.congtacgiaovien.TenCT, "", new { @class = "form-control tencongtacadd", @placeholder = "Tên công tác giáo viên", required = "required" })
                                                        </div>
                                                    </div>
                                                    <div class="form-group">
                                                        <label class="col-md-12"> Flag </label>
                                                        <div class="col-md-12">
                                                            @Html.DropDownListFor(Model => Model.congtacgiaovien.Flag, new SelectList(ViewBag.Trangthai, "id", "flag"), new { @id = "congtacadd_flag", @class = "form-control flagcongtacadd", required = "required" })
                                                        </div>
                                                    </div>
                                                </div>
                                            </div>


                                            <div class="modal-footer">

                                                <button type="button" class="btn btn-default" data-dismiss="modal">Đóng</button>
                                                <button type="button" id="btnthemcongtac" class="btn btn-success waves-effect waves-light m-r-30">Thêm</button>

                                            </div>
                                        </form>


                                    </div>

                                </div>
                            </div>
                            <div id="suacongtac" class="modal fade" role="dialog">
                                <div class="modal-dialog  modal-lg">

                                    <!-- Modal content-->
                                    <div class="modal-content">
                                        <div class="modal-header">
                                            <button type="button" class="close" data-dismiss="modal">&times;</button>
                                            <h4 class="modal-title">Sửa công tác giáo viên</h4>
                                        </div>

                                        <form data-toggle="validator" class="form-horizontal">
